label setFortune
f[0]=abs(rand)%100
f[1]=strat("黒赤緑青黄桃紫白金銀灰",abs(rand)%11)
luckCent=abs(rand)%101
f[2]=strat("大中小 ",abs(rand)%4)+strat("凶吉",luckCent/51)
