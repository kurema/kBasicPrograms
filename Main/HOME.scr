label multiMenu
p=0
mesl=messageLength
winx=mwinx winy=mwiny
for i=0 to messageLength
mes[i]=message[i]
next
gosub"stdMenu"
p[0]=p
if not(message[p][0]=0 or p=-1)then
winx=mwinx+strwidth("s")
winy=mwiny+(strheight+3)*(p*2+1)/2+2
for i=1 to message[p][0]
mesl=message[p][0]-1
mes[i-1]=message[p][i]
next
p=0
gosub"stdMenu"
p[1]=p
if p[1]=-1 then gosub"multiMenu"endif
endif
for i=0 to messageLength
message[i][0]=0
next
return

