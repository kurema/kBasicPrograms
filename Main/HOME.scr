gosub"renew"
while not(ipt and keyselect)
color(bgr,bgg,bgb)
frect(objx/2-100,objy/2-strheight,200,strheight*2)
color(lnr,lng,lnb)
rect(objx/2-100,objy/2-strheight,200,strheight*2)
